{
  "name": "shared-background",
  "version": "1.0.0",
  "description": "Share background between multiple children",
  "main": "dist/shared-background.min.js",
  "scripts": {
    "build:js": "rollup src/shared-background.js -c -o dist/shared-background.min.js",
    "build:jquery": "rollup src/shared-background.jquery.js -c -o dist/shared-background.jquery.min.js",
    "build": "npm run build:js & npm run build:jquery",
    "watch:js": "npm run build:js -- -w",
    "watch:jquery": "npm run build:jquery -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyJobCompany/shared-background.git"
  },
  "keywords": [
    "vanilla",
    "jquery",
    "plugin",
    "background"
  ],
  "author": "MyJob.Company Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MyJobCompany/shared-background/issues"
  },
  "homepage": "https://github.com/MyJobCompany/shared-background#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
